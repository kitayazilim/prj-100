      </footer>
      <!-- main-footer end -->

      <!--Scroll to top-->
      <button class="scroll-top scroll-to-target" data-target="html">
        <span class="far fa-angle-up"></span>
      </button>
    </div>

    <!-- jequery plugins -->
    <script src="{{ '/assets/js/jquery.js' | url }}"></script>
    <script src="{{ '/assets/js/popper.min.js' | url }}"></script>
