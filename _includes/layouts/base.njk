    <link href="{{ '/assets/css/jquery.fancybox.min.css' | url }}" rel="stylesheet">
    <link href="{{ '/assets/css/animate.css' | url }}" rel="stylesheet">
    <link href="{{ '/assets/css/rtl.css' | url }}" rel="stylesheet">
    <link href="{{ '/assets/css/color.css' | url }}" rel="stylesheet">
    <link href="{{ '/assets/css/style.css' | url }}" rel="stylesheet">
    <link href="{{ '/assets/css/responsive.css' | url }}" rel="stylesheet">

  </head>

  <!-- page wrapper -->
  <body>

    <div class="boxed_wrapper">

      <!-- preloader -->
      <div class="loader-wrap">
        <div class="preloader">
          <div class="preloader-close">Loaderı Kapat</div>
          <div id="handle-preloader" class="handle-preloader">
            <div class="animation-preloader">
              <img src="{{'/assets/images/loader.png' | url}}" alt="" style="width:35% ;position:absolute; left:33%; top:13%">
              <div class="spinner"></div>
              <div class="txt-loading">
                <span data-text-preloader="a" class="letters-loading">
                  a
                </span>
                <span data-text-preloader="s" class="letters-loading">
                  s
                </span>
                <span data-text-preloader="b" class="letters-loading">
                  b
                </span>
                <span data-text-preloader="a" class="letters-loading">
                  a
                </span>
                <span data-text-preloader="l" class="letters-loading">
                  l
                </span>
              </div>
            </div>
          </div>
        </div>
      </div>
      <!-- preloader end -->

      <!-- main header -->
      <header class="main-header style-two">
        <!-- header-lower -->
        <div class="header-lower">
          <div class="outer-box">
            <div class="logo-box">
              <figure class="logo">
                <a href="/"><img src="{{'/assets/images/logo-3.png' | url}}" alt=""></a>
              </figure>
            </div>
            <div class="menu-area">
              <!--Mobile Navigation Toggler-->
              <div class="mobile-nav-toggler">
                <i class="icon-bar"></i>
                <i class="icon-bar"></i>
                <i class="icon-bar"></i>
              </div>
              <nav class="main-menu navbar-expand-md navbar-light">
                <div class="collapse navbar-collapse show clearfix" id="navbarSupportedContent">
                  <ul class="navigation clearfix">
                    {% set navPages = collections.all | eleventyNavigation %}
                    {%- for item in navPages -%}
                      <li>
                        <a href="{{item.url | url}}" class="lang" key="{{item.aka}}">{{item.title}}</a>
                      </li>
                    {%- endfor -%}
                  </ul>
                </div>
              </nav>
            </div>
            <div class="nav-right-content d-flex">
              <ul class="social-links clearfix d-none d-md-block">
                {%- for social in sitedata.socials -%}
                  <li>
                    <a href="{{social.url | url}}" title="{{social.name}}" rel="noreferrer" target="_blank">
                      <i class="fab {{social.icon}}"></i>
                    </a>
                  </li>
                {%- endfor -%}
              </ul>
              <ul class="menu-right-content clearfix">
                <li style="margin: 0 2px">
                  <a class="translate" id="de" style="cursor:pointer"><img src="{{'/assets/images/de.png' | url}}" width="30px" alt="de"></a>
                </li>
                <li style="margin: 0 2px">
                  <a class="translate" id="tr" style="cursor:pointer"><img src="{{'/assets/images/tr.png' | url}}" width="30px" alt="tr"></a>
                </li>
                <li style="margin: 0 2px">
                  <a class="translate" id="en" style="cursor:pointer"><img src="{{'/assets/images/en.png' | url}}" width="30px" alt="en"></a>
                </li>
              </ul>
            </div>

          </div>
        </div>

        <!--sticky Header-->
        <div class="sticky-header">
          <div class="outer-box">
            <div class="logo-box">
              <figure class="logo">
                <a href="/"><img src="{{'/assets/images/logo.png'| url}}" alt=""></a>
              </figure>
            </div>
            <div class="menu-area">
              <nav class="main-menu clearfix">
                <!--Keep This Empty / Menu will come through Javascript-->
              </nav>
            </div>
            <div class="nav-right-content d-flex">
              <ul class="social-links clearfix d-none d-md-block">
                {%- for social in sitedata.socials -%}
                  <li>
                    <a href="{{social.url | url}}" title="{{social.name}}" rel="noreferrer" target="_blank">
                      <i class="fab {{social.icon}}"></i>
                    </a>
                  </li>
                {%- endfor -%}
              </ul>
              <ul class="menu-right-content clearfix">
                <li style="margin: 0 2px">
                  <a class="translate" id="de" style="cursor:pointer"><img src="{{'/assets/images/de.png' | url}}" width="30px" alt="de"></a>
                </li>
                <li style="margin: 0 2px">
                  <a class="translate" id="tr" style="cursor:pointer"><img src="{{'/assets/images/tr.png' | url}}" width="30px" alt="tr"></a>
                </li>
                <li style="margin: 0 2px">
                  <a class="translate" id="en" style="cursor:pointer"><img src="{{'/assets/images/en.png' | url}}" width="30px" alt="en"></a>
                </li>
                <li style="margin: 0 2px">
                  <a class="translate" id="ar" style="cursor:pointer"><img src="{{'/assets/images/ar.png' | url}}" width="30px" alt="ar"></a>
                </li>
              </ul>
            </div>
          </div>
        </div>
      </header>
      <!-- main-header end -->

      <!-- Mobile Menu -->
      <div class="mobile-menu">
        <div class="menu-backdrop"></div>
        <div class="close-btn">
          <i class="fas fa-times"></i>
        </div>

        <nav class="menu-box">
          <div class="nav-logo">
            <a href="/"><img src="{{'/assets/images/logo.png' | url}}" alt="" title=""></a>
          </div>
          <div class="menu-outer">
            <!--Here Menu Will Come Automatically Via Javascript / Same Menu as in Header-->
          </div>
          <div class="contact-info">
            <h4 class="lang" key="bizeulasin">{{collections.contact[0].data.bizeulasin}}</h4>
            <ul>
              <li>{{sitedata.adres}}</li>
              <li>
                <a href="tel:{{sitedata.tel}}">{{sitedata.tel}}</a>
              </li>
              <li>
                <a href="mailto:{{sitedata.email}}">{{sitedata.email}}</a>
              </li>
            </ul>
          </div>
          <div class="social-links">
            <ul class="clearfix">
              {%- for social in sitedata.socials -%}
                <li>
                  <a href="{{social.url | url}}" title="{{social.name}}" rel="noreferrer" target="_blank">
                    <span class="fab {{social.icon}}"></span>
                  </a>
                </li>
              {%- endfor -%}
            </ul>
          </div>
        </nav>
      </div>
      <!-- End Mobile Menu -->

      {{content | safe}}

      <!-- main-footer -->
