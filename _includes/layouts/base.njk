                      {%- endfor -%}
                    </ul>
                    <div class="call-box">
                      <p>
                        <i class="fal fa-phone"></i>
                        <a href="tel:{{sitedata.tel}}">{{sitedata.tel}}</a>
                      </p>
                      <p>
